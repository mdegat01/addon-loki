{
  "name": "Loki",
  "url": "https://github.com/mdegat01/addon-loki",
  "version": "edge",
  "slug": "loki",
  "arch": ["aarch64", "amd64", "armv7"],
  "description": "Loki for Home Assistant",
  "startup": "system",
  "map": ["share", "ssl"],
  "watchdog": "[PROTO:ssl]://[HOST]:[PORT:3100]/ready",
  "ports": {
    "3100/tcp": null
  },
  "ports_description": {
    "3100/tcp": "HTTP listen port"
  },
  "options": {
    "ssl": false,
    "log_level": "info"
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str?",
    "keyfile": "str?",
    "cafile": "str?",
    "config_path": "str?",
    "log_level": "list(debug|info|warning|error)?"
  }
}
